<?xml version="1.0" encoding="utf-8"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Name for the robot -->
  <xacro:arg name="robot_id" default="M92C" />

  <!-- End-effector id" -->
  <xacro:arg name="hand_id" default="m92c_hand" />

  <!-- Should self-collision be enabled? -->
  <xacro:arg name="with_sc" default="false" />

  <!-- Should fake sensors be used? -->
  <xacro:arg name="description_pkg" default="human_description" />

</robot>